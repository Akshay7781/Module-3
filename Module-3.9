//Write a program to find out the max number from given 10 elements of array
#include<stdio.h>
void main()
{
    int a[10],i,max;
    printf("enter array elements : ");
   
    for(i=0;i<=10;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[10];
    for(i=0;i<=10;i++)
    {
        if (a[i]>max)
        {
            max=a[i];
        }
    }
    printf("max value of array is=%d",max);
}