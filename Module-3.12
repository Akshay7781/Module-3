//Write a program to find out the Max number from given Matrix 
#include<stdio.h>
void main()
{
	int a[3][3],i,j,max;
	printf("enter the elements=");
	for(i=0;i<3;i++)
	{
	    for(j=0;j<3;j++)
	    {
	        scanf("%d",&a[i][j]);
	        if (a[i][j]>max)
	        max=a[i][j];
	    }
	}
	printf("\nmax number of given matrix is =%d",max);
}